DROP TABLE IF EXISTS `blog_author_about`;
CREATE TABLE `blog_author_about` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `top_img` varchar(150) NOT NULL DEFAULT '' COMMENT '头像',
  `name` varchar(15) NOT NULL DEFAULT '' COMMENT '昵称',
  `introduce` varchar(50) NOT NULL DEFAULT '' COMMENT '介绍',
